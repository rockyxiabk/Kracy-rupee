<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape >

            <solid android:color="#ffb400" />

            <corners
                android:radius="5dp"/>

            <stroke
                android:width="1dp"
                android:color="#ffb400"
                android:dashWidth="5dp" />

        </shape>
    </item>
    <item android:state_enabled="false">
        <shape >

            <solid android:color="#cbcbcb" />

            <corners
                android:radius="5dp"/>

            <stroke
                android:width="1dp"
                android:color="#cbcbcb"
                android:dashWidth="5dp" />

        </shape>
    </item>
    <item android:state_enabled="true">
        <shape >

            <solid android:color="#ffb400" />

            <corners
                android:radius="5dp"/>

            <stroke
                android:width="1dp"
                android:color="#ffb400"
                android:dashWidth="5dp" />

        </shape>
    </item>
</selector>